{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "linebreak-style": [2, "unix"],
    "max-len": [2, 120],
    "no-console": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"]
  }
}
